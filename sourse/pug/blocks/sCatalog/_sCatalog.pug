mixin tab()
	.tabs__content.tabs-inner__content&attributes(attributes)
		.row
			block
mixin tabBtn(t)
	.tabs__btn.tabs-inner__btn&attributes(attributes)= t

mixin card(i,p)
	+e.col.col-xl-3.col-lg-4.col-6
		.card().text-center
			+pic(`prod-${i}.png`,'',0)
			//- img.card-img-top(src=`` alt= p)

			p.card-text= p
			a.btn.btn-primary.w-100(href='#modal-call' data-title=`Запросить цену` data-order=`Запросить цену: ${p}`).link-modal-js Запросить цену
