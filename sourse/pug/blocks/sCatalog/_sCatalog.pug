mixin sCatalogCard(data)
	.col-6.col-xl-4&attributes(attributes)
		+e.card
			if data.linkHref
				+e.doc-btn(href=data.linkHref target="_blank" data-hint="Документация")
					+icon('doc')
			+e.img
				img(loading="lazy" src=`img/@2x/sCatalogCard-${data.imgGroup}-${data.imgNum}.png` alt="")
			+e.title=data.title
			+e.tags
				-for(let tag of data.tags)
					+e.tag=tag
			+e.btn.link-modal-js(href="#modal-call" data-title="Запросить цену" data-order=`Запросить цену: ${data.title}`) Запросить цену

mixin sCatalogDDitem(data)
	+e.dd-item(class=`${data.addClasses || ''} ${data.addItemClasses || ''}`)
		//-+e.dd-head its tabs(cTabs) btn and dd-head, its class 'active' dont have any style it styled by dd-item.active
		//-two js integrations that works indepenttly from each other
		+e.dd-head.dd-head-js.cTabs-btn-js(class=`${data.addClasses || ''}`)=data.title
		if block
			+e.dd-content.dd-content-js(class=`${data.addClasses || ''}`)
				block

mixin sCatalogDDChb(txt)
	+e.LABEL.chb-item
		input(type="checkbox" class="invisible" name=txt)&attributes(attributes)
		.square
		.chb-txt=txt